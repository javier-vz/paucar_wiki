SELECT ?item ?itemLabel ?itemDescription ?tipo ?tipoLabel ?propiedad ?propiedadLabel ?valor ?valorLabel {
  # Buscar por tipos específicos de entidades culturales en Perú
  VALUES ?tipo { 
    wd:Q200538    # fiesta
    wd:Q375011    # festividad religiosa
    wd:Q4579447   # peregrinación
    wd:Q131036    # danza
  }
  ?item wdt:P31 ?tipo;
        wdt:P17 wd:Q419.  # En Perú

  # Obtener propiedades clave
  ?item ?propiedad ?valor.
  FILTER(STRSTARTS(STR(?propiedad), "http://www.wikidata.org/prop/direct/"))
  FILTER(?propiedad IN (wdt:P31, wdt:P17, wdt:P131, wdt:P276, wdt:P361, wdt:P138))
  SERVICE wikibase:label { bd:serviceParam wikibase:language "es,en,qu,ay,[AUTO_LANGUAGE]". }
}
LIMIT 500